{
    "luahelper.format.align_args": true,
    "luahelper.format.align_parameter": true,
    "luahelper.format.break_before_table_rb": false,
    "luahelper.format.break_after_table_lb": false,
    "luahelper.format.chop_down_kv_table": true,
    "luahelper.format.spaces_inside_table_braces": true,
    "luahelper.format.double_quote_to_single_quote": true,
    "todo-tree.regex.regex": "(--|//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
    "Lua.workspace.library": [
        "${3rd}/lmaobox-annotation/library"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.completion.autoRequire": false,
    "explorer.sortOrder": "type",
    "Lua.workspace.ignoreDir": [
        ".vscode",
        "lua_modules",
        "lmaobox_modules"
    ],
}